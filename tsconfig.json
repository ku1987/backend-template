{
  "compilerOptions": {
    "module": "es2015",
    "lib": ["es6","dom"],
    "outDir": "lib",
    "rootDir": "/",
    "strict": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
  }
}